{
    "rewrites": [
      {"source": "/(.*)", "destination": "/"}
    ],
    "headers": [
      {
        "source": "/build/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
  
